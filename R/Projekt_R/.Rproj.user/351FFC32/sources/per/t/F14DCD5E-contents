mon<-as.factor(c("sty","lut","mar","kwi","maj","cze","lip","sie","wrz","paź","lis","gru"), levels = c(1:12))
# oznaczenie miesięcy;
mon<-factor(c("sty","lut","mar","kwi","maj","cze","lip","sie","wrz","paź","lis","gru"), levels = c(1:12))
