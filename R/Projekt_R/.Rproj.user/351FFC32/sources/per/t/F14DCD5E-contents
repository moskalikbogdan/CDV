# oznaczenie miesięcy;
mon<-factor(mies, levels=c("sty","lut","mar","kwi","maj","cze","lip","sie","wrz","paź","lis","gru"), ordered = TRUE)
# tworzenie ramki danych z danymi
dane<-data.frame(woj=woj1, rok=rok1, mies=mies, mon=mon, miesoWolZKosciazaKg=miesoWolZKosciazaKg, serDojrzewZaKg=serDojrzewZaKg,
jajaKurze=jajaKurze, masło=Masło, jablkaZaKg=jablkaZaKg, czyszczenieGarnituru=CzyszczenieGarnituru,
polbutyDamskie=PolbutyDamskie, bateriaZlewoz=BateriaZlewoz, wizytaULekarza=WizytaULekarza)
malopolska <- dane[dane$woj == "MAŁOPOLSKIE",]
str(malopolska)
# porównanie po latach
ggplot(data = malopolska,
aes(x = mon,
y = miesoWolZKosciazaKg,
color =rok,
group=rok)) +
geom_point(size = 5,
shape = 15) +
ggtitle("Miesiąc v. cena Mięsa Wołowego z kością dla małopolski") +
scale_x_discrete( labels = c("sty","lut","mar","kwi","maj","cze","lip","sie","wrz","paź","lis","gru")) +
theme(axis.text.x = element_text(angle = 90, hjust = .5))
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg)) + geom_boxplot()
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg)) +
geom_boxplot(alpha=0.3) +
theme(legend.position="none") +
scale_fill_brewer(palette="Dark2")
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg, fill=mon)) +
geom_boxplot(alpha=0.3)
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg, fill=mon)) +
geom_boxplot(alpha=0.3) +
scale_fill_brewer(palette="Dark2")
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg, fill=mies)) +
geom_boxplot(alpha=0.3)
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg, fill=mies)) +
geom_boxplot(alpha=0.5)
ggplot(malopolska, aes(x=mon, y=miesoWolZKosciazaKg, fill=mies)) +
geom_boxplot(alpha=0.6)
